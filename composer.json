{
	"name" : "viames/pair_boilerplate",
	"description" : "Skeleton project based on Pair PHP framework",
	"type" : "project",
	"minimum-stability" : "stable",
	"prefer-stable": true,
	"require": {
		"viames/pair": "^1.9"
	},
	"repositories" : [
		{
			"type" : "vcs",
			"url" : "https://github.com/viames/pair_boilerplate"
		}
	],
	"license" : "MIT",
	"keywords" : [
		"pair",
		"php",
		"multilingual",
		"framework",
		"active-record",
		"plugin-system",
		"pagination",
		"router",
		"rest-api"
	],
	"homepage" : "https://github.com/viames/pair_boilerplate",
	"authors" : [
		{
			"name" : "Viames Marino",
			"role" : "Developer"
		}
	],
	"config": {
		"allow-plugins": {
			"php-http/discovery": true
		},
		"platform": {
			"php": "8.3"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^11"
	}
}
